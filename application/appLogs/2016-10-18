 LEFT JOIN `user_login` AS `ul` ON ri.destination_id = ul.user_id WHERE (mi.source_id = \'1\' ) AND (mi.is_deleted IS NULL OR ri.is_deleted = 0) AND (mi.is_draft = 1) GROUP BY `mi`.`mail_id` ORDER BY `mi`.`sent_on` DESC'
